<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<servlet>
		<servlet-name>file</servlet-name>
		<servlet-class>com.interior.community.control.UploadController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>file</servlet-name>
		<url-pattern>/file/*</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>homeparty</servlet-name>
		<servlet-class>com.interior.community.control.HomepartyController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>homeparty</servlet-name>
		<url-pattern>/homeparty/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>qna</servlet-name>
		<servlet-class>com.interior.community.control.QnaController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>qna</servlet-name>
		<url-pattern>/qna/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>home</servlet-name>
		<servlet-class>com.interior.community.control.HomeController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>home</servlet-name>
		<url-pattern>/index.do</url-pattern>
	</servlet-mapping>
</web-app>